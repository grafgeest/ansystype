  "license": "MIT",
  "homepage": "https://gitlab.com/mkryuk/ansystype#readme",
  "devDependencies": {
    "@types/angular": "^1.6.9",
    "@types/angular-ui-router": "^1.1.36",
    "@types/systemjs": "^0.20.2",
    "concurrently": "^3.4.0",
    "lite-server": "^2.3.0",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.2",
    "bootstrap": "^3.3.7",
    "systemjs": "^0.19.9"
  }
